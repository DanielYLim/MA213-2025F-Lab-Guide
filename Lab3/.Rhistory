library(tidyverse)
set.seed(100)
N = 96
extran_n = 4
#### A for box plot
A = rnorm(N, 0,1)
A = append(A, c(5,4,3.5,4.2))
A <- sample(A) # randomize A
N = N+extran_n
seq_N = seq(N)
df <- data.frame(N=seq_N, A=A)
df %>% ggplot() + geom_boxplot(aes(A))
boxplot(df$A)
######## B, C for scatter plot
B = seq(-5,4.9, 0.1)
C = B^2
df <- cbind(df, B,C)
plot(df$B , df$C)
